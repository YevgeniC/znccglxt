        <typeAlias alias="Library" type="se.zust.entity.Library"/>
<!--         <typeAlias alias="Inlibrary" type="se.zust.entity.Inlibrary"/> -->
        <typeAlias alias="Outlibrary" type="se.zust.entity.Outlibrary"/>
    </typeAliases>

    
</configuration>
        